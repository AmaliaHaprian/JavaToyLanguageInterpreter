<AnchorPane prefHeight="541.0" prefWidth="686.0" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.mapgui.ViewProgramController">
   <children>
      <VBox layoutX="55.0" layoutY="22.0" prefHeight="480.0" prefWidth="553.0">
         <children>
